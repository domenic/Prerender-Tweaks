{
  "name": "Prerender Tweaks",
  "manifest_version": 3,
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "main.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_top.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "action": {
    "default_title": "PrerenderTweeks"
  }
}